		{
			"name": "GetBookingId",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"// assertion\r",
							"\r",
							"var status_code=pm.response.code;\r",
							"console.log(status_code);\r",
							"\r",
							"if(status_code==200){\r",
							"var responsedata = pm.response.json();\r",
							"console.log(responsedata)\r",
							"\r",
							"pm.test(\"First Name Check\",function(){\r",
							"\r",
							"       pm.expect(pm.environment.get(\"fname\")).to.eql(responsedata.firstname)\r",
							"})\r",
							"\r",
							"pm.test(\"Last Name Check\",function(){\r",
							"\r",
							"       pm.expect(pm.environment.get(\"lname\")).to.eql(responsedata.lastname)\r",
							"})\r",
							"//manually check\r",
							"pm.test(\"Total Price check\",function(){\r",
							"       pm.expect(pm.environment.get(\"price_product\")).to.eql(responsedata.totalprice.toString())\r",
							"})\r",
							"\r",
							"pm.test(\"Deposit paid\",function(){\r",
							"       pm.expect(pm.environment.get(\"is_depositpaid\")).to.eql(responsedata.depositpaid.toString())\r",
							"})\r",
							"\r",
							"pm.test(\"Check in\",function(){\r",
							"       pm.expect(pm.environment.get(\"check_in\")).to.eql(responsedata.bookingdates.checkin)\r",
							"})\r",
							"\r",
							"pm.test(\"Check out\",function(){\r",
							"       pm.expect(pm.environment.get(\"check_out\")).to.eql(responsedata.bookingdates.checkout)\r",
							"})\r",
							"\r",
							"pm.test(\"Additional Needs\", function(){\r",
							"\r",
							"       pm.expect(pm.environment.get(\"is_additionalneeds\")).to.eql(responsedata.additionalneeds)\r",
							"})\r",
							"\r",
							"\r",
							"\r",
							"}\r",
							"\r",
							"else if(status_code==400){\r",
							"       pm.test(\"ID expired\");\r",
							"}\r",
							"\r",
							"else if(status_code==401){\r",
							"       pm.test(\"Token Expired\");\r",
							"}\r",
							"else{\r",
							"     pm.test(\"Something went wrong\");\r",
							"}"
						],
						"type": "text/javascript",
						"packages": {},
						"requests": {}
					}
