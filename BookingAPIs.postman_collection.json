		{
			"name": "CreateBooking",
			"event": [
				{
					"listen": "prerequest",
					"script": {
						"exec": [
							"var firstname=pm.variables.replaceIn(\"{{$randomFirstName}}\");\r",
							"////console.log(firstname);\r",
							"pm.environment.set(\"fname\",firstname);\r",
							"\r",
							"\r",
							"var lastname=pm.variables.replaceIn(\"{{$randomLastName}}\");\r",
							"//console.log(lastname);\r",
							"pm.environment.set(\"lname\",lastname);\r",
							"\r",
							"var price_product = pm.variables.replaceIn(\"{{$randomInt}}\");\r",
							"console.log(price_product);\r",
							"pm.environment.set(\"price_product\", price_product);\r",
							"\r",
							"var depositpaid = pm.variables.replaceIn(\"{{$randomBoolean}}\");\r",
							"console.log(depositpaid);\r",
							"pm.environment.set(\"is_depositpaid\", depositpaid);\r",
							"\r",
							"var additionalneeds = pm.variables.replaceIn(\"{{$randomJobArea}}\");\r",
							"console.log(additionalneeds);\r",
							"pm.environment.set(\"is_additionalneeds\",additionalneeds );\r",
							"\r",
							"\r",
							"const in_date=require('moment');\r",
							"// console.log(in_date);\r",
							"const checkin_date=in_date();\r",
							"// console.log(checkin_date);\r",
							"var checkin_dateres=checkin_date.format(\"YYYY-MM-DD\");\r",
							"//console.log(checkin_dateres)\r",
							"pm.environment.set(\"check_in\",checkin_dateres);\r",
							"\r",
							"\r",
							"const out_date=require('moment');\r",
							"// console.log(in_date);\r",
							"const checkout_date=out_date();\r",
							"// console.log(checkin_date);\r",
							"var checkout_dateres2=checkout_date.add(4,'d').format(\"YYYY-MM-DD\");\r",
							"//console.log(checkin_dateres)\r",
							"pm.environment.set(\"check_out\",checkout_dateres2);\r",
							"\r",
							"\r",
							""
						],
						"type": "text/javascript",
						"packages": {},
						"requests": {}
					}
				},
